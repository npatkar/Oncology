<h1>Listing article_sections</h1>

<table>
  <tr>
  </tr>

<% for article_section in @article_sections %>
  <tr>
    <td><%= link_to article_section.display_name, :action=>'edit', :id=>article_section.id%></td>
    <td><%= link_to 'Remove', article_section, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New article_section', new_article_section_path %>
